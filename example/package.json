{
  "name": "react-native-vision-camera-example",
  "description": "Example app for react-native-vision-camera",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "setup": "cd ios && bundle install",
    "pods": "cd ios && bundle exec pod install",
    "lint": "eslint .",
    "lint-ci": "yarn lint -f ../node_modules/@firmnav/eslint-github-actions-formatter/dist/formatter.js",
    "typescript": "tsc --noEmit",
    "pod": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install && cd .."
  },
  "dependencies": {
    "@react-native-community/blur": "^4.3.2",
    "@react-native-camera-roll/camera-roll": "^5.7.2",
    "@react-native-community/slider": "^4.1.8",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.2.3",
    "react": "^18.2.0",
    "react-native": "^0.72.0-rc.6",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-pressable-opacity": "^1.0.4",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.21.0",
    "react-native-static-safe-area-insets": "^2.2.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-video": "https://github.com/react-native-yeouido/react-native-video.git#feature/new-arch-fabric"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@tsconfig/react-native": "^3.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@types/metro-config": "^0.76.3",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.65.5",
    "@types/react-native-vector-icons": "^6.4.9",
    "@types/react-native-video": "^5.0.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "typescript": "4.8.4"
  }
}